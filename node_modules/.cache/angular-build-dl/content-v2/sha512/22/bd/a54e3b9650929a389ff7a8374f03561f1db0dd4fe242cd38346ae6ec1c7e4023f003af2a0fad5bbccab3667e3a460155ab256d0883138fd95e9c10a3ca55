ght</mat-icon>\r\n            <span (click)=\"OpenDetailDialog(4)\">예측 선형 함수 그리기</span>\r\n            <mat-icon>keyboard_arrow_right</mat-icon>\r\n            <span (click)=\"OpenDetailDialog(5)\">x = 70 값 예측해보기</span>\r\n        </div>\r\n        <!--    <section class=\"thumbnails\">-->\r\n        <!--      <div class=\"code\">-->\r\n        <!--        <div *ngFor=\"let cd of codeArray; let i = index\">-->\r\n        <!--          <span [id]=\"'code' + i\" [innerText]=\"cd + '\\n'\"></span>-->\r\n        <!--        </div>-->\r\n        <!--      </div>-->\r\n\r\n        <!--      <div class=\"flowchart\">-->\r\n        <!--        <span>predict Data</span>-->\r\n        <!--        <div class=\"predict\">-->\r\n        <!--          <input id=\"predict-input\" type=\"text\" placeholder=\"input x\" [(ngModel)]=\"predictX\"/>-->\r\n        <!--          <button (click)=\"predict()\">predict</button>-->\r\n        <!--        </div>-->\r\n\r\n        <!--        <div class=\"prediction\">-->\r\n        <!--          <span>output Prediction : </span>-->\r\n        <!--          <span>{{prediction}}</span>-->\r\n        <!--        </div>-->\r\n\r\n        <!--        <span>add Data</span>-->\r\n        <!--        <div class=\"add-input\">-->\r\n        <!--          <input id=\"input-x\" type=\"text\" placeholder=\"input x\" [(ngModel)]=\"inputX\"/>-->\r\n        <!--          <input id=\"input-y\" type=\"text\" placeholder=\"input y\" [(ngModel)]=\"inputY\"/>-->\r\n        <!--          <button (click)=\"add()\">add</button>-->\r\n        <!--        </div>-->\r\n\r\n        <!--        <svg id=\"flowchart\" class=\"svg\"></svg>-->\r\n\r\n        <!--        <div class=\"button\">-->\r\n        <!--          <button (click)=\"test()\">test</button>-->\r\n        <!--          <button (click)=\"clear()\">clear</button>-->\r\n        <!--        </div>-->\r\n        <!--      </div>-->\r\n\r\n        <!--      <div class=\"vis\">-->\r\n        <!--        <img id=\